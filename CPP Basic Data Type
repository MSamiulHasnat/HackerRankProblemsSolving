https://www.hackerrank.com/challenges/c-tutorial-basic-data-types/problem?isFullScreen=true

Solve-

#include <iostream>
#include <stdio.h>
#include <cstdio>
using namespace std;

int main() {

    int a;
    long long b;
    char c;
    float d;
    double e;

    scanf("%d %lld %c %f %lf", &a, &b, &c, &d, &e);
    
    printf("%d\n%lld\n%c\n%.3f\n%.9f\n", a,b,c,d,e);


    return 0;
}
